{
   "version": 2,
   "builds": [
     {
       "src": "index.js",
       "use": "@now/node"
     },
     {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./server.js",
      "use": "@now/node-server"
    }
   ],
   "routes": [{"handle": "filesystem"},
     {
       "src": "/.*",
       "dest": "./routes/controllers.js"
     },
     {
      "src": "/.*",
      "dest": "/api/"
     }
   ]
}