{
   "version": 2,
   "builds": [
     {
       "src": "index.js",
       "use": "@now/node"
     },
     {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./server.js",
      "use": "@now/node-server"
    }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "index.js"
     }
   ]
}